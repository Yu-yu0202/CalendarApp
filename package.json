        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "newlines-between": "always"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "build/",
      "src/*"
    ]
  }
