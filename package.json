  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2023": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "newlines-between": "always"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "build/",
      "src/*"
    ]
  }
