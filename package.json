{
  "name": "calendarapp",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/Yu-yu0202/CalendarApp#readme",
  "bugs": {
    "url": "https://github.com/Yu-yu0202/CalendarApp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yu-yu0202/CalendarApp.git"
  },
  "license": "BSD-3-Clause",
  "author": "Yu-yu0202",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev"
  },
  "dependencies": {
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2023": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "newlines-between": "always"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "build/",
      "src/"
    ]
  }
}
