  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2023": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
